<h1 class="title">{{this.name}}</h1>
<div class='content-container'>
  <ul class="list">
    <li class="list__item">Capital: {{this.capital}}</li>
    <li class="list__item">Population: {{this.population}}</li>
    <li class="list__item">Languages:
      <ul>
        {{#each this.languages}}

          <li class="list__item-lang">{{this.name}}</li>

        {{/each}}
      </ul>
    </li>

  </ul>
  <img width="500px" src='{{this.flag}}' alt='{{this.name}}' />
</div>